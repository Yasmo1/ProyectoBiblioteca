{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}
{% block field %}
    <div>
        {% if object.image != null %}
            <img width="100" height="100" src="{{ asset('files/imagesSitiosReferencia/' ~ object.image) }}">
        {% else %}
            <span>No picture</span>
        {% endif %}
    </div>
{% endblock %}